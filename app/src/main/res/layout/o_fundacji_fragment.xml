<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/oFundacji"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.main.OFundacji">


    <TextView
        android:id="@+id/header_o_fundacji"
        android:layout_width="183dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/roboto_condensed_regular"
        android:text="@string/header_o_fundacji"
        android:textColor="@color/colorTitle"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/pasekill"
        android:layout_width="300dp"
        android:layout_height="20dp"
        android:layout_marginStart="@dimen/activity_horizontal_margin"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="101dp"
        android:src="@drawable/pasekill"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/header_o_fundacji" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="203dp"
        android:layout_height="31dp"
        android:layout_marginStart="19dp"
        android:layout_marginTop="32dp"
        android:text="@string/kim_jestesmy"
        android:textColor="@color/colorSubTitle"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pasekill" />

</androidx.constraintlayout.widget.ConstraintLayout>
