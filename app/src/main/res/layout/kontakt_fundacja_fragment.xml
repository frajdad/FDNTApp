<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/oFundacji"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainFrame">


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelinetop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.05" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineleft"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.05" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineright"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.95" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelinemiddle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.81" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineicons"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.19" />

    <TextView
        android:id="@+id/header_fundacja"
        android:layout_width="183dp"
        android:layout_height="0dp"
        android:fontFamily="@font/roboto_condensed_regular"
        android:text="@string/header_fundacja"
        android:textColor="@color/colorTitle"
        android:textSize="28sp"
        app:autoSizeTextType="uniform"
        app:layout_constraintDimensionRatio="h,5:1"
        app:layout_constraintStart_toStartOf="@id/guidelineleft"
        app:layout_constraintTop_toTopOf="@+id/guidelinetop" />


    <include
        android:id="@+id/pasekill"
        layout="@layout/pasekill"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="@id/guidelineright"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@id/guidelineleft"
        app:layout_constraintTop_toBottomOf="@+id/header_fundacja" />


    <ImageView
        android:id="@+id/ikonatrasa"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/lok"
        app:layout_constraintBottom_toTopOf="@+id/ikonatel"
        app:layout_constraintEnd_toEndOf="@+id/guidelineicons"
        app:layout_constraintHeight_percent="0.14"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@id/guidelineleft"
        app:layout_constraintTop_toBottomOf="@+id/pasekill"
        app:layout_constraintWidth_percent="0.14" />

    <ImageView
        android:id="@+id/ikonatel"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/tel"
        app:layout_constraintBottom_toTopOf="@+id/ikonamail"
        app:layout_constraintEnd_toEndOf="@+id/guidelineicons"
        app:layout_constraintHeight_percent="0.14"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@id/guidelineleft"
        app:layout_constraintTop_toBottomOf="@+id/ikonatrasa"
        app:layout_constraintWidth_percent="0.14" />

    <ImageView
        android:id="@+id/ikonamail"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/mail"
        app:layout_constraintBottom_toTopOf="@+id/ikonanip"
        app:layout_constraintEnd_toEndOf="@+id/guidelineicons"
        app:layout_constraintHeight_percent="0.14"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@id/guidelineleft"
        app:layout_constraintTop_toBottomOf="@+id/ikonatel"
        app:layout_constraintWidth_percent="0.14" />

    <ImageView
        android:id="@+id/ikonanip"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/nip"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/guidelineicons"
        app:layout_constraintHeight_percent="0.14"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@id/guidelineleft"
        app:layout_constraintTop_toBottomOf="@+id/ikonamail"
        app:layout_constraintWidth_percent="0.14" />

    <TextView
        android:id="@+id/adres"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="38dp"
        android:text="@string/adres"
        app:autoSizeTextType="uniform"
        app:layout_constraintHeight_percent="0.03"
        app:layout_constraintStart_toEndOf="@+id/ikonatrasa"
        app:layout_constraintTop_toTopOf="@+id/ikonatrasa"
        app:layout_constraintWidth_percent="0.30" />

    <TextView
        android:id="@+id/adres_tekst"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="9dp"
        android:text="@string/adres_tekst"
        android:textColor="@color/colorSubTitle"
        app:autoSizeTextType="uniform"
        app:layout_constraintHeight_percent="0.09"
        app:layout_constraintStart_toEndOf="@+id/ikonatrasa"
        app:layout_constraintTop_toBottomOf="@+id/adres"
        app:layout_constraintWidth_percent="0.60" />

    <TextView
        android:id="@+id/telefon"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="36dp"
        android:text="@string/tel"
        app:autoSizeTextType="uniform"
        app:layout_constraintHeight_percent="0.03"
        app:layout_constraintStart_toEndOf="@+id/ikonatel"
        app:layout_constraintTop_toTopOf="@+id/ikonatel"
        app:layout_constraintWidth_percent="0.30" />

    <TextView
        android:id="@+id/tel_tekst"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="11dp"
        android:text="@string/tel_tekst"
        android:textColor="@color/colorSubTitle"
        app:autoSizeTextType="uniform"
        app:layout_constraintHeight_percent="0.04"
        app:layout_constraintStart_toEndOf="@+id/ikonatel"
        app:layout_constraintTop_toBottomOf="@+id/telefon"
        app:layout_constraintWidth_percent="0.25" />

    <TextView
        android:id="@+id/mail"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="36dp"
        android:text="@string/email"
        app:autoSizeTextType="uniform"
        app:layout_constraintHeight_percent="0.03"
        app:layout_constraintStart_toEndOf="@+id/ikonamail"
        app:layout_constraintTop_toTopOf="@+id/ikonamail"
        app:layout_constraintWidth_percent="0.30" />

    <TextView
        android:id="@+id/mail_tekst"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="11dp"
        android:text="@string/mail_tekst"
        android:textColor="@color/colorSubTitle"
        app:autoSizeTextType="uniform"
        app:layout_constraintHeight_percent="0.035"
        app:layout_constraintStart_toEndOf="@+id/ikonamail"
        app:layout_constraintTop_toBottomOf="@+id/mail"
        app:layout_constraintWidth_percent="0.35" />

    <TextView
        android:id="@+id/nip"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="36dp"
        android:text="@string/nip"
        app:autoSizeTextType="uniform"
        app:layout_constraintHeight_percent="0.03"
        app:layout_constraintStart_toEndOf="@+id/ikonanip"
        app:layout_constraintTop_toTopOf="@+id/ikonanip"
        app:layout_constraintWidth_percent="0.30" />

    <TextView
        android:id="@+id/nip_tekst"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="12dp"
        android:text="@string/nip_tekst"
        android:textColor="@color/colorSubTitle"
        app:autoSizeTextType="uniform"
        app:layout_constraintHeight_percent="0.04"
        app:layout_constraintStart_toEndOf="@+id/ikonanip"
        app:layout_constraintTop_toBottomOf="@+id/nip"
        app:layout_constraintWidth_percent="0.25" />


</androidx.constraintlayout.widget.ConstraintLayout>